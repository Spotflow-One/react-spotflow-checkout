{
  "name": "react-spotflow-checkout",
  "version": "0.1.01",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "yarn build:modern",
    "build:vite:lib": "tsc --p ./tsconfig.build.json && vite build",
    "build:watch": "yarn build:modern -w",
    "lint": "eslint . --ext ts,tsx,js --report-unused-disable-directives --max-warnings 0",
    "build:modern": "rollup --bundleConfigAsCjs -c ./scripts/rollup/rollup.config.js",
    "build:esm": "rollup --bundleConfigAsCjs -c ./scripts/rollup/rollup.esm.config.js",
    "build:vite": "yarn clean && tsc && vite build",
    "build:dist": "rollup --bundleConfigAsCjs -c",
    "lint-yarn": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "type": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky",
    "postversion": "git push && git push origin v$npm_package_version",
    "prepublishOnly": "yarn install && yarn lint:fix && yarn type && yarn build"
  },
  "peerDependencies": {
    "clsx": "^2.1.1",
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.5 || ^17.0.0 || ^18.0.0",
    "tailwind-merge": "^2.3.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react-otp-input": "^3.1.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^5.0.8",
    "rollup": "^4.18.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "yarn@4.3.1+sha512.af78262d7d125afbfeed740602ace8c5e4405cd7f4735c08feb327286b2fdb2390fbca01589bfd1f50b1240548b74806767f5a063c94b67e431aabd0d86f7774"
}
